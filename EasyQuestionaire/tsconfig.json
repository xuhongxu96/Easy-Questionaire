{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [ "webpack-env" ],
    "downlevelIteration": true,
    "lib": ["dom", "es5", "es2015.collection", "es2015.iterable", "es2015"]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
